@using SailbotVSv3.ViewModels
@model WindViewModel

<h1>@Model.title</h1>
<h2></h2>

@{
    foreach (var wind in Model.winds)
    {
        <div>Sensor ID: @wind.WindDirection</div>
        <br />
        <div>Voltage: @wind.Voltage.</div>
        <br />
        <div>Update Time: @wind.Temperature</div>
        <br />
        <h1>@wind.UpdatedTime</h1>
    }
}
